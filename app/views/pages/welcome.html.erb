<html>

<head>

  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/css/materialize.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.1.0/css/all.css" integrity="sha384-lKuwvrZot6UHsBSfcMvOkWwlCMgc0TaWr+30HWe3a4ltaBwTZhyTEggF5tJv8tbt"
    crossorigin="anonymous">
  <link rel="stylesheet" type="text/css" href="style.css">
  <title>Friend It</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body id="home" class="scrollspy">
  
     

  <section class="slider">
    <ul class="slides">
      <li>
        <img src="https://image.ibb.co/hbEMux/resort1.jpg" alt="">
        <!-- random image -->
        <div class="caption center-align">
          <h2>Friend It</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only"></h5>
        </div>
      </li>
      <li>
        <img src="https://image.ibb.co/mn1egc/resort2.jpg" alt="">
        <!-- random image -->
        <div class="caption left-align">
          <h2>Your Community First Rental Solutions</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only"></h5>
        </div>
      </li>
      <li>
        <img src="https://image.ibb.co/mbCVnH/resort3.jpg" alt="">
        <!-- random image -->
        <div class="caption right-align">
          <h2>Ready?</h2>
          <h5 class="light grey-text text-lighten-3 hide-on-small-only"></h5>
        </div>
      </li>
      <li>
    </ul>
  </section>

  <!--Section: Search-->
  <section id="search" class="section section-search teal darken-1 white-text center scrollspy">
    <div class="container">
      <div class="row">
        <div class="col s12">
          <h3>How Does It Work?</h3>
        </div>
      </div>
    </div>
  </section>

  <!--Section: Icon Boxes-->
  <section class="section section-icons grey lighten-4 center">
    <div class="container">
      <div class="row">
        <div class="col s12 m4">
          <!--entire screen when on mobile, but 4 columns on medium size and above-->
          <div class="card-panel">
            <i class="material-icons large teal-text">account_circle</i>
            <h4>Sign Up</h4>
            <p>Be part of our Community!</p>
          </div>
        </div>
        <div class="col s12 m4">
          <div class="card-panel">
            <i class="material-icons large teal-text">group_add</i>
            <h4>Add Your Friends</h4>
            <p>Time to add some of you best buddies</p>
          </div>
        </div>
        <div class="col s12 m4">
          <div class="card-panel">
            <i class="material-icons large teal-text">home</i>
            <h4>Upload Your Flat</h4>
            <p>Have a flat? Share it with your friends</p>
          </div>
        </div>
        <div class="col s12 m4">
          <div class="card-panel">
            <i class="material-icons large teal-text">verified_user</i>
            <h4>Done!</h4>
            <p>You can share properties with your friends!</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!--Section: Popular Places-->
  <section id="popular" class="secion section-popular scrollspy">
    <div class="container">
      <div class="row">
        <h4 class="center">
          <span class="teal-text">Check Out <br> </span>Some Flats</h4>
        <div class="col s12 m4">
          <div class="card">
            <div class="card-image">
              <img src="https://image.ibb.co/hbEMux/resort1.jpg" alt="">
              <span class="card-title">Rome, Italy</span>
            </div>
            <div class="card-content">Lorem ipsum dolor sit amet consectetur, adipisicing elit. Maxime, quos!
            </div>
          </div>
        </div>
        <div class="col s12 m4">
          <div class="card">
            <div class="card-image">
              <img src="https://image.ibb.co/mn1egc/resort2.jpg" alt="">
              <span class="card-title">Berlin, Germany</span>
            </div>
            <div class="card-content">Lorem ipsum dolor sit amet consectetur, adipisicing elit. Maxime, quos!
            </div>
          </div>
        </div>
        <div class="col s12 m4">
          <div class="card">
            <div class="card-image">
              <img src="https://image.ibb.co/mbCVnH/resort3.jpg" alt="">
              <span class="card-title">Paris, France</span>
            </div>
            <div class="card-content">Lorem ipsum dolor sit amet consectetur, adipisicing elit. Maxime, quos!
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!--Section: Follow-->
  <section class="section section-follow teal darken-2 white-text center">
    <div class="container">
      <div class="row">
        <div class="col s12">
          <h4>Follow Friend It</h4>
          <p>Follow us on social media</p>
          <a href="#" class="white-text">
            <i class="fab fa-facebook fa-4x"></i>
          </a>
          <a href="#" class="white-text">
            <i class="fab fa-twitter fa-4x"></i>
          </a>
          <a href="#" class="white-text">
            <i class="fab fa-linkedin fa-4x"></i>
          </a>
          <a href="#" class="white-text">
            <i class="fas fa-envelope fa-4x"></i>
          </a>
          <a href="#" class="white-text">
            <i class="fab fa-github fa-4x"></i>
          </a>
        </div>
      </div>
    </div>
  </section>

  <!--Section: Gallery-->
  <section id="gallery" class="section section-gallery scrollspy">
    <div class="container">
      <h4 class="center">
        <span class="teal-text">Photo</span> Gallery
      </h4>
      <div class="row">
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?beach" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?travel" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?nature" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?beach, travel" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?beaches" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?traveling" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?bridge" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?boat, travel" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?water" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?building" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?trees" alt="" class="materialboxed responsive-img">
        </div>
        <div class="col s12 m3">
          <img src="https://source.unsplash.com/1600x900/?cruise" alt="" class="materialboxed responsive-img">
        </div>
      </div>
    </div>
  </section>

  <!--Section: Contact-->
  <section id="contact" class="section section-contact scrollspy">
    <div class="container">
      <div class="row">
        <div class="col s12 m6">
          <div class="card-panel teal white-text center">
            <i class="material-icons">directions_run</i>
            <h5>Ready to Go?</h5>
            <br>
            <p><%=link_to "sign up", new_user_registration_path, class: "btn btn-ghost"%></p>
          </div>
          
        </div>
        
    </div>
  </section>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0-rc.2/js/materialize.min.js"></script>
  <script type="text/javascript" src="script.js"></script>
</body>
</html>
























